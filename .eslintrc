{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "react-hooks",
        "jsx-yara11y",
        "jest"
    ],
    "env": {
        "browser": true,
        "es2022": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "json": true
        }
    },
    "rules": {
        "quotes": ["error", "single"],
        "react/no-unescaped-entities": "off"
    },
    "ignorePatterns": ["*.json"]
}
