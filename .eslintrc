{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2022": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "json": true
        }
    },
    "rules": {
        "quotes": ["error", "single"]
    },
    "ignorePatterns": ["*.json"]
}
